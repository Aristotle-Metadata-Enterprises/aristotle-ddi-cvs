            <Value ColumnRef="Term">
                <ComplexValue>
                    <ddi-cv:Value xml:lang="en-US">Average absolute deviation</ddi-cv:Value>
                </ComplexValue>
            </Value>
            <Value ColumnRef="Definition">
                <ComplexValue>
                    <ddi-cv:Value xml:lang="en-US">The average of the absolute differences between each value and the overall mean. Measure of statistical dispersion around the mean, alternative to Standard Deviation.</ddi-cv:Value>
                </ComplexValue>
            </Value>
        </Row>
        <Row>
            <Value ColumnRef="Code">
                <SimpleValue>MedianAbsoluteDeviation</SimpleValue>
            </Value>
            <Value ColumnRef="Term">
                <ComplexValue>
                    <ddi-cv:Value xml:lang="en-US">Median absolute deviation</ddi-cv:Value>
                </ComplexValue>
            </Value>
            <Value ColumnRef="Definition">
                <ComplexValue>
                    <ddi-cv:Value xml:lang="en-US">The median absolute deviation from the median. Measure of statistical dispersion around the median.</ddi-cv:Value>
                </ComplexValue>
            </Value>
        </Row>
